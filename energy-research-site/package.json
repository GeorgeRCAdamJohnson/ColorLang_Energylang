{
  "name": "energy-research-showcase",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "lint:strict": "eslint . --config .eslintrc.prevention.js --ext ts,tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit",
    "type-check:strict": "tsc --noEmit --project tsconfig.strict.json",
    "pre-commit": "tsx scripts/pre-commit-ts-check.ts",
    "validate": "npm run type-check:strict && npm run lint:strict && npm run test --run",
    "generate-seo": "tsx scripts/generate-seo-files.ts",
    "audit": "tsx scripts/audit-performance.ts",
    "prebuild": "npm run generate-seo",
    "postbuild": "npm run audit"
  },
  "dependencies": {
    "chart.js": "^4.4.0",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.5",
    "glob": "^10.3.10",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.294.0",
    "papaparse": "^5.4.1",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^6.20.1"
  },
  "devDependencies": {
    "@headlessui/react": "^1.7.17",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^13.4.0",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.8",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.10.0",
    "@types/papaparse": "^5.3.14",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/react-helmet-async": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.1.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.0",
    "tailwindcss": "^3.3.6",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.2",
    "typescript": "^5.2.2",
    "vite": "^7.3.0"
  }
}
